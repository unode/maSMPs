{
  "@context": {
      "schema": "http://schema.org/",
      "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
      "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
      "maSMP": "https://discovery.biothings.io/view/maSMP/"
  },
  "@graph": [
     {
          "@id": "maSMP:Question",
          "@type": "rdfs:Class",
          "rdfs:comment": "A specific question - e.g. from a user seeking answers online, or collected in e.g., a Frequently Asked Questions (FAQ) document, survey or management plan.",
          "rdfs:label": "Question",
          "rdfs:subClassOf": {
              "@id": "schema:Question"
          },
          "schemaVersion": [
              "https://schema.org/docs/releases.html#v23.0"
          ]
      },
     {
          "@id": "maSMP:expectedMetadata",
          "@type": "rdf:Property",
          "rdfs:comment": "Metadata statement (domain, property and range) or property (if the domain and range are irrelevant) associated to this question. It does not necessarily provide a direct answer to the question but it would be related to it. In the case of mng plans, such a metadata term is expected to exist by the end of the mng plan. ",
          "rdfs:label": "expectedMetadata",
          "schema:domainIncludes": {
            "@id": "maSMP:Question"
          },
          "schema:rangeIncludes": [
             {
                  "@id": "rdf:Statement"
             },
             {
                  "@id": "schema:DefinedTerm"
             },
             {
                  "@id": "schema:PropertyValue"
             }
          ]
      }
  ]
}