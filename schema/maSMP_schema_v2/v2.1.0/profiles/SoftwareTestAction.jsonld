{
  "@context": {
    "schema": "http://schema.org/",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "softestaction": "https://discovery.biothings.io/view/softestaction/"
  },
  "@graph": [
    {
      "@id": "softestaction:SoftwareTestAction",
      "@type": "rdfs:Class",
      "rdfs:comment": "The act of testing a software according to its specifications. For instance, testing a software with a particular testType using a specific testInput and getting a specific testOutput (aka result).",
      "rdfs:label": "SoftwareTestAction",
      "rdfs:subClassOf": {
        "@id": "maSMP:SoftwareTestAction"
      },
      "$validation": {
        "$schema": "http://json-schema.org/draft-07/schema#",
        "type": "object",
        "properties": {
          "softwareTested": {
            "description": "Link to the software tested by this action.",
            "owl:cardinality": "one"
          },
          "testInput": {
            "description": "Input used to performed the test. Some tests may not require any input, some may require multiple ones. If the order is important in the case of multiple inputs, a ListItem could help.",
            "owl:cardinality": "many"
          },
          "testType": {
            "description": "The type of test that it is performed on the object.",
            "owl:cardinality": "one"
          },
          "result": {
            "description": "The result produced in the action. e.g. John wrote *a book*.",
            "owl:cardinality": "many"
          },
          "target": {
            "description": "Indicates a target EntryPoint for an Action.",
            "owl:cardinality": "many"
          },
          "description": {
            "description": "A description of the item.",
            "owl:cardinality": "one"
          }
        },
        "required": [
          "softwareTested",
          "testType"
        ],
        "recommended": [
          "result",
          "description"
        ],
        "optional": [
          "testInput",
          "target"
        ]
      },
      "schemaVersion": [
        "https://schema.org/docs/releases.html#v23.0"
      ]
    }
  ]
}